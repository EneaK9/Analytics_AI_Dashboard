# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# OpenAI API Configuration for AI data analysis
OPENAI_API_KEY=your_openai_api_key

# JWT Secret for authentication (generate a secure random string)
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Superadmin credentials (change in production)
SUPERADMIN_USERNAME=admin
SUPERADMIN_PASSWORD=change_this_password

# Application Settings
ENVIRONMENT=production
DEBUG=false

# Frontend URL for CORS (your Render frontend URL)
FRONTEND_URL=https://your-frontend-app.onrender.com

# Optional: Legacy Pipedream (can be removed)
PIPEDREAM_WEBHOOK_URL=
PIPEDREAM_CLIENT_ID=
PIPEDREAM_CLIENT_SECRET=
PIPEDREAM_PROJECT_ID= 